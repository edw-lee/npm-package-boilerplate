{
    "compilerOptions": {
      "module": "ESNext",
      "esModuleInterop": true,
      "target": "es5",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "jsx": "react-jsx",
      "declaration": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "lib": [
        "dom",
        "dom.iterable",
        "esnext",
        "ES2021.String"
      ]
    },
    "include": [
      "src"
    ],
  }